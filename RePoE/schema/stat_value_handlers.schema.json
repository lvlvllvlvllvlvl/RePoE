{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "type": "object",
    "additionalProperties": {
        "oneOf": [
            {
                "$ref": "#/definitions/IntHandler"
            },
            {
                "$ref": "#/definitions/RelationalData"
            },
            {
                "$ref": "#/definitions/CanonicalLine"
            },
            {
                "$ref": "#/definitions/Noop"
            }
        ]
    },
    "title": "StatValueHandlersSchema",
    "definitions": {
        "IntHandler": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "addend": {
                    "type": "number"
                },
                "divisor": {
                    "type": "number"
                },
                "multiplier": {
                    "type": "number"
                },
                "precision": {
                    "type": "integer"
                },
                "fixed": {
                    "type": "boolean"
                },
                "type": {
                    "type": "string",
                    "enum": [
                        "int"
                    ]
                }
            },
            "required": [
                "type"
            ],
            "title": "IntHandler"
        },
        "RelationalData": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "dat_file": {
                    "type": "string"
                },
                "index_column": {
                    "type": "string"
                },
                "type": {
                    "type": "string",
                    "enum": [
                        "relational"
                    ]
                },
                "value_column": {
                    "type": "string"
                },
                "values": {
                    "type": "object",
                    "patternProperties": {
                        "^\\d+$": {
                            "type": "string"
                        }
                    }
                },
                "predicate": {
                    "$ref": "#/definitions/Predicate"
                }
            },
            "required": [
                "type",
                "dat_file",
                "value_column",
                "values"
            ],
            "title": "RelationalData"
        },
        "Predicate": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "column": {
                    "type": "string"
                },
                "value": true
            },
            "title": "Predicate"
        },
        "CanonicalLine": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "type": {
                    "type": "string",
                    "enum": [
                        "string"
                    ]
                }
            },
            "required": [
                "type"
            ],
            "title": "CanonicalLine"
        },
        "Noop": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "type": {
                    "type": "string",
                    "enum": [
                        "noop"
                    ]
                }
            },
            "required": [
                "type"
            ],
            "title": "Noop"
        }
    }
}
